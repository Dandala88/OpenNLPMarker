{
  "manifest_version": 2,
  "name": "OpenNLPMarker",
  "description": "Testing Plugin",
  "version": "1.0",
  "content_scripts":[
    {
        "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],"browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}